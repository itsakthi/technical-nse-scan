<div style="display:inline-grid;width:15%">
  <p><%= optionDataColl[0].underlyingPrice %></p>
  <p><%= optionDataColl[0].date %></p>
  <p><%= optionDataColl[0].callOpenInterestChangePer %></p>
  <p><%= optionDataColl[0].putOpenInterestChangePer %></p>
  <div style="display:inline-grid;width:5%">
    <% for(var i=0; i < (optionDataColl[0].optionData.length)/2; i++) {%>
      <p><%= (optionDataColl[0].optionData[i].price).toFixed(2) %></p>
    <% } %>
  </div>
  <div style="display:inline-grid;width:5%">
    <% for(var j=0; j < (optionDataColl[0].optionData.length)/2; j++) {%>
      <p><%= optionDataColl[0].optionData[j].strikePrice %></p>
    <% } %>
  </div>
  <div style="display:inline-grid;width:5%">
    <% for(; i < optionDataColl[0].optionData.length; i++) {%>
      <p><%= (optionDataColl[0].optionData[i].price).toFixed(2) %></p>
    <% } %>
  </div>
</div>
